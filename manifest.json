{
	"manifest_version": 2,
	
	"name": "OHS Chrome Extension",
	"description": "This extension auto-logs you into the OHS school wifi.",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"webRequest",
		"background",
		"webRequestBlocking",
		"*://172.31.0.2/*"
	],
	"browser_action": {
		"default_icon": "icon.png"
  }
}